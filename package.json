{
  "name": "react-media-ui",
  "version": "0.0.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "style.css"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && npm run css",
    "delete-css": "rimraf ./style.css",
    "create-css": "cat src/image.css src/video.css > style.css",
    "css": "npm run delete-css && npm run create-css",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepublishOnly": "npm run build",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "author": "James Please",
  "module": "dist/react-media-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-media-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-media-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^2.7.1",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "core-hooks": "^1.3.0-beta.0"
  }
}
